{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"inlineSources": false,
		"declaration": true,
		"stripInternal": false,
		"preserveConstEnums": true,
		"noEmitOnError": true,
		"pretty": true,
		"inlineSourceMap": false,

		"removeComments": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,

		"lib": [ "es6" ],
		"outDir": "../lib"
	}
}